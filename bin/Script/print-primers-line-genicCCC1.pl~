# get row contan numbers of filds.
use strict;
my $input = $ARGV[0];
my @parray;
my $minus = "-";
open( PFILE, "<$input" );
while (<PFILE>) {
    chomp();
    if (/\t(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)\s+(\S+)(\s+)/) {
        my $id   = $1;
        my $rnu   = $2;
        my $rtyp   = $3;
        my $rseq = $4;
         my $rsiz   = $5;
        my $rstart   = $6;
         my $rend   = $7;
         my $rend8   = $8;
         my $rend9   = $9;
         my $rend10   = $10;
         my $rend11   = $11;
         my $rend12   = $12;
         my $rend13   = $13;
         my $rend14   = $14;
         my $rend15   = $15;
         my $rend16   = $16;
 
         print "\t$id\t$rnu\t$rtyp\t$rseq\t$rsiz\t$rstart\t$minus\t$minus\t$rend9\t$rend10\t$rend12\t$rend13\t$rend14\t$rend15\t$rend16\n";
       }
         } 
close GFILE;
